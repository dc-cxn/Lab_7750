<configure xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <configure xmlns="urn:nokia.com:sros:ns:yang:sr:conf" xmlns:nokia-attr= "urn:nokia.com:sros:ns:yang:sr:attributes">
    <port>
      <port-id>{int_name}</port-id>
      <description>{int_desc}</description>
      <admin-state>true</admin-state>
      <ethernet>
        <mode>{int_mode}</mode>
      </ethernet>  
    </port>
  </configure>
</configure>